{
  "ConnectionStrings": {
    "SqlConnection": "YourUserSecretKey:ConnectionStrings:SqlConnection",
    "RedisConnection": "YourUserSecretKey:ConnectionStrings:RedisConnection"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },

  "ElasticConfiguration": {
    "Uri": "http://localhost:9200/"
  },


  "Token": {
    "Key": "YourUserSecretKey:Token:Key",
    "Issuer": "YourUserSecretKey:Token:Issuer",
    "AccessTokenExpireInDays": 1,
    "RefreshTokenExpireInDays": 7,
    "LogoutToken": -1
  },

  "PasswordAllRoles": {
    "Password": "YourUserSecretKey:PasswordAllRoles:Password"
  },

  "AllowedHosts": "*"

}
